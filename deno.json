{
  "imports": {
    "hono": "jsr:@hono/hono@^4.8.3",
    "kysely": "npm:kysely@^0.28.1",
    "kysely-ctl": "npm:kysely-ctl@^0.13.1",
    "kysely-codegen": "npm:kysely-codegen",
    "pg": "npm:pg",
    "pg-pool": "npm:pg-pool"
  },
  "tasks": {
    "start": "deno run --env-file --allow-env --allow-net ./src/main.ts",
    "kysely-codegen": "deno run -A npm:kysely-codegen --singularize --out-file ./src/db/models.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  }
}
